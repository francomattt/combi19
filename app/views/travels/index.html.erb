<p id="notice"><%= notice %></p>

<h1>Travels</h1>

<table>
  <thead>
    <tr>
      <th>Travel day</th>
      <th>Travel hour</th>
      <th>Combi</th>
      <th>Way</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travels.each do |travel| %>
      <tr>
        <td><%= travel.travel_day %></td>
        <td><%= travel.travel_hour %></td>
        <td><%= travel.combi_id %></td>
        <td><%= travel.way_id %></td>
        <td><%= travel.state_id %></td>
        <td><%= link_to 'Show', travel %></td>
        <td><%= link_to 'Edit', edit_travel_path(travel) %></td>
        <td><%= link_to 'Destroy', travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travel', new_travel_path %>
